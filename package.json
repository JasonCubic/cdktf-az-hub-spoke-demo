{
  "name": "cdktf_az_hub_spoke_demo",
  "version": "1.0.0",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "type": "module",
  "license": "MPL-2.0",
  "private": true,
  "scripts": {
    "dev": "rimraf dist && npm run copy-files && env-cmd tsc-watch --project . --outDir ./dist --onSuccess \"nodemon --watch 'dist' ./dist/index.js\"",
    "get": "cdktf get",
    "copy-files": "copyfiles -u 1 src/**/*.template src/**/*.sh dist/",
    "build": "rimraf dist && tsc && npm run copy-files",
    "synth": "env-cmd cdktf synth",
    "plan:spoke": "env-cmd cdktf plan spoke-demo",
    "plan:hub": "env-cmd cdktf plan hub-demo",
    "plan:peerHubAndSpoke": "env-cmd cdktf plan peer-hub-demo-and-spoke-demo",
    "plan:renameHubSubscription": "env-cmd cdktf plan rename-hub-subscription",
    "plan:renameSpokeSubscription": "env-cmd cdktf plan rename-spoke-subscription",
    "plan:singleSubHubAndSpoke": "env-cmd cdktf plan single-sub-hub-and-spoke",
    "plan:addPeeringRoleToHubAndSpoke": "env-cmd cdktf plan add-peering-role-to-hub-and-spoke",
    "apply:spoke": "env-cmd cdktf deploy --auto-approve spoke-demo",
    "apply:hub": "env-cmd cdktf deploy --auto-approve hub-demo",
    "apply:addPeeringRoleToHubAndSpoke": "env-cmd cdktf deploy --auto-approve add-peering-role-to-hub-and-spoke",
    "apply:peerHubAndSpoke": "env-cmd cdktf deploy --auto-approve --ignore-missing-stack-dependencies peer-hub-demo-and-spoke-demo",
    "apply:fullHubAndSpoke": "env-cmd cdktf deploy --auto-approve spoke-demo hub-demo add-peering-role-to-hub-and-spoke peer-hub-demo-and-spoke-demo && npm run destroy:addPeeringRoleToHubAndSpoke",
    "apply:renameHubSubscription": "env-cmd cdktf deploy --auto-approve rename-hub-subscription",
    "apply:renameSpokeSubscription": "env-cmd cdktf deploy --auto-approve rename-spoke-subscription",
    "apply:singleSubHubAndSpoke": "env-cmd cdktf deploy --auto-approve single-sub-hub-and-spoke",
    "destroy:spoke": "env-cmd cdktf destroy spoke-demo --auto-approve",
    "destroy:hub": "env-cmd cdktf destroy hub-demo --auto-approve",
    "destroy:addPeeringRoleToHubAndSpoke": "env-cmd cdktf destroy --auto-approve --ignore-missing-stack-dependencies add-peering-role-to-hub-and-spoke",
    "destroy:peerHubAndSpoke": "env-cmd cdktf destroy --auto-approve --ignore-missing-stack-dependencies peer-hub-demo-and-spoke-demo",
    "destroy:fullHubAndSpoke": "env-cmd cdktf destroy --auto-approve spoke-demo hub-demo peer-hub-demo-and-spoke-demo add-peering-role-to-hub-and-spoke",
    "destroy:singleSubHubAndSpoke": "env-cmd cdktf destroy --auto-approve single-sub-hub-and-spoke",
    "tf:init:hub": "env-cmd terraform -chdir=./cdktf.out/stacks/hub-demo init",
    "tf:plan:hub": "env-cmd terraform -chdir=./cdktf.out/stacks/hub-demo plan",
    "tf:apply:hub": "env-cmd terraform -chdir=./cdktf.out/stacks/hub-demo apply --auto-approve",
    "tf:state:pull:hub": "env-cmd terraform -chdir=./cdktf.out/stacks/hub-demo state pull",
    "tf:destroy:hub": "env-cmd terraform -chdir=./cdktf.out/stacks/hub-demo destroy --auto-approve",
    "test": "jest",
    "test:watch": "jest --watch",
    "tfsec:cdktfDemo": "tfsec ./cdktf.out/stacks/cdktf-demo",
    "tfsec:singleSubHubAndSpoke": "tfsec ./cdktf.out/stacks/single-sub-hub-and-spoke",
    "infracost": "cdktf synth && infracost breakdown --path ./cdktf.out",
    "act": "rimraf cdktf.out dist && act"
  },
  "engines": {
    "node": ">=14.0"
  },
  "dependencies": {
    "@cdktf/provider-azurerm": "^4.0.6",
    "cdktf": "^0.14.3",
    "constructs": "^10.1.215"
  },
  "devDependencies": {
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.17",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.0.0",
    "copyfiles": "^2.4.1",
    "env-cmd": "^10.1.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsc-watch": "^6.0.0",
    "typescript": "^4.9.3"
  }
}
